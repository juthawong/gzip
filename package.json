{
  "name": "mako-gzip",
  "version": "1.0.0",
  "description": "A mako plugin for compressing files using gzip",
  "keywords": [
    "mako-plugin",
    "gzip",
    "compression"
  ],
  "repository": "makojs/gzip",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "defaults": "^1.0.3",
    "mako-utils": "^1.0.0"
  },
  "peerDependencies": {
    "mako": "^1.0.0"
  },
  "devDependencies": {
    "buffer-equal": "^1.0.0",
    "chai": "^3.2.0",
    "istanbul": "^0.4.3",
    "istanbul-coveralls": "^1.0.3",
    "mako": "^1.0.0",
    "mocha": "^3.0.0",
    "standard": "^8.0.0",
    "uid": "0.0.2"
  },
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "test": "istanbul cover _mocha",
    "coveralls": "istanbul-coveralls",
    "preversion": "npm test",
    "version": "git-changelog -t $npm_package_version && git add HISTORY.md",
    "postversion": "git push && git push --tags"
  },
  "standard": {
    "ignore": [
      "test/fixtures/**",
      "example/**"
    ]
  }
}
